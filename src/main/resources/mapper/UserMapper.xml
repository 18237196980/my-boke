<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bo.ke.myboke.mapper.UserMapper">

    <insert id="insertUser" keyProperty="id" useGeneratedKeys="true" parameterType="com.bo.ke.myboke.entity.User">
        insert into
        user
        (id,username)
        values
        (#{id},#{username})
    </insert>

</mapper>
